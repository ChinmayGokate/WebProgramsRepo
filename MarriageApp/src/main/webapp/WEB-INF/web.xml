
	<web-app>
		<servlet>
			<servlet-name>marriage</servlet-name>
			<servlet-class>com.chinmay.servlet.MarriageApplication</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>marriage</servlet-name>
			<url-pattern>/marriageurl</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>welcome</servlet-name>
